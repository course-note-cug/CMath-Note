\begin_layout Subsection
向量对向量
\end_layout

\begin_layout Enumerate
常数相关
\end_layout

\begin_deeper
\begin_layout Enumerate
常向量的导数是 0: 若
\begin_inset Formula $\bf a$
\end_inset

中不含变量
\begin_inset Formula $\bf x$
\end_inset

, 那么
\begin_inset Formula $\frac{\pd\bf a}{\pd\bf x}=\bf 0.$
\end_inset


\end_layout

\begin_layout Enumerate
常数进出求导: 若
\begin_inset Formula $a$
\end_inset

中不含变量
\begin_inset Formula $\bf x$
\end_inset

, 
\begin_inset Formula $\bf{u=u}(\bf x)$
\end_inset

那么
\begin_inset Formula $\frac{\pd a\bf u}{\pd\bf x}=a\frac{\pd\bf u}{\pd\bf x}.$
\end_inset


\end_layout

\begin_layout Enumerate
常矩阵进出求导: 
\end_layout

\begin_deeper
\begin_layout Enumerate
若
\begin_inset Formula $\bf A\in\R^{n\times m}$
\end_inset

中不含变量
\begin_inset Formula $\bf x\in\R^{m\times1}$
\end_inset

, 那么
\begin_inset Formula $\frac{\pd\bf{Ax}}{\pd\bf x}=\bf A.$
\end_inset


\end_layout

\begin_layout Enumerate
若
\begin_inset Formula $\bf A\in\R^{n\times m}$
\end_inset

中不含变量
\begin_inset Formula $\bf x\in\R^{m\times1}$
\end_inset

, 那么
\begin_inset Formula $\frac{\pd\bf{x'A}}{\pd\bf x}=\bf{A'}.$
\end_inset


\end_layout

\begin_layout Enumerate
若
\begin_inset Formula $\bf A\in\R^{n\times m}$
\end_inset

中不含变量
\begin_inset Formula $\bf x\in\R^{m\times1},$
\end_inset

而向量值函数
\begin_inset Formula $\bf{u=u(x)}\in\R^{m\times1}$
\end_inset

, 那么
\begin_inset Formula $\frac{\pd\bf{Au}}{\pd\bf x}=\bf A.$
\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Enumerate
乘法
\end_layout

\begin_deeper
\begin_layout Enumerate
变元在向量: 
\begin_inset Formula $a$
\end_inset

中不含
\begin_inset Formula $\bf x$
\end_inset

, 但是
\begin_inset Formula $\bf u=\bf u(\bf x)$
\end_inset

: 
\begin_inset Formula $\frac{\pd a\bf u}{\partial\bf x}=a\frac{\pd\bf u}{\pd\bf x}$
\end_inset

.
\end_layout

\begin_layout Enumerate
变元在常数: 
\begin_inset Formula $v=v(\bf x),\bf a$
\end_inset

中不含
\begin_inset Formula $\bf x:$
\end_inset


\begin_inset Formula $\frac{\pd v\bf a}{\pd\bf x}=\bf a\frac{\pd v}{\pd\bf x}.$
\end_inset


\end_layout

\begin_layout Enumerate
两个都有: 
\begin_inset Formula $v=v(\bf x),\bf{u=u(x)}$
\end_inset

: 
\begin_inset Formula $\frac{\pd v\bf u}{\pd\bf x}=v\frac{\partial\bf u}{\partial\bf x}+\bf u\frac{\pd v}{\pd\bf x}.$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
加法: 
\begin_inset Formula $\bf{u=u(x),v=v(x)},$
\end_inset

那么
\begin_inset Formula $\frac{\pd(\bf{u+v)}}{\pd\bf x}=\frac{\pd\bf u}{\pd\bf x}+\frac{\pd\bf v}{\pd\bf x}$
\end_inset

.
\end_layout

\begin_layout Enumerate
链式法则
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\bf{u=u(x),g=g(u)},$
\end_inset


\begin_inset Formula $\frac{\pd\bf g}{\pd\bf x}=\frac{\pd\bf g}{\pd\bf u}\frac{\pd\bf u}{\pd\bf x}$
\end_inset

.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\bf{u=u(x),f=f(g),g=g(u)},$
\end_inset


\begin_inset Formula $\frac{\pd\bf f}{\pd\bf x}=\frac{\pd\bf f}{\pd\bf g}\frac{\pd\bf g}{\pd\bf u}\frac{\pd\bf u}{\pd\bf x}$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Subsection
标量对向量
\end_layout

\begin_layout Enumerate
常数相关: 
\begin_inset Formula $a$
\end_inset

中不含
\begin_inset Formula $\bf x$
\end_inset

, 
\begin_inset Formula $u=u(\bf x).$
\end_inset

 
\begin_inset Formula $\frac{\pd au}{\pd\bf x}=a\frac{\pd u}{\pd\bf x}.$
\end_inset


\end_layout

\begin_layout Enumerate
加法: 
\begin_inset Formula $u=u(\bf x),v=v(\bf x).$
\end_inset

 
\begin_inset Formula $\frac{\pd(u+v)}{\pd\bf x}=\frac{\pd u}{\pd\bf x}+\frac{\pd v}{\pd\bf x}$
\end_inset

.
\end_layout

\begin_layout Enumerate
乘法: 如果
\begin_inset Formula $u=u(\bf x),v=v(\bf x),$
\end_inset

 
\begin_inset Formula $\frac{\pd uv}{\pd\bf x}=u\frac{\pd v}{\pd\bf x}+v\frac{\pd u}{\pd\bf x}$
\end_inset

.
\end_layout

\begin_layout Enumerate
链式法则: 
\begin_inset Formula $f=f(g),g=g(u),u=u(\bf x),$
\end_inset


\begin_inset Formula $\frac{\pd f}{\pd\bf x}=\frac{\pd f}{\pd g}\frac{\pd g}{\pd u}\frac{\pd u}{\pd\bf x}.$
\end_inset


\end_layout

\begin_layout Enumerate
点积: 
\begin_inset Formula $\bf{u=u(x),v=v(x),}$
\end_inset


\begin_inset Formula $\frac{\pd(\bf{u\cdot v})}{\pd\bf x}=\bf u'\frac{\pd\bf v}{\pd\bf x}+\bf v'\frac{\pd\bf u}{\pd\bf x}$
\end_inset

.
\end_layout

\begin_layout Enumerate
二次型: 
\begin_inset Formula $\bf{u=u(x),v=v(x),A}$
\end_inset

中不含
\begin_inset Formula $\bf x,$
\end_inset


\begin_inset Formula $\frac{\pd(\bf{u'Av})}{\pd\bf x}=\bf u'\bf A\frac{\pd\bf v}{\pd\bf x}+\bf v'\bf A'\frac{\pd\bf u}{\pd\bf x}$
\end_inset

.
\end_layout

\begin_layout Subsection
向量对标量
\end_layout

\begin_layout Enumerate
常数相关: 
\end_layout

\begin_deeper
\begin_layout Enumerate
常数: 
\begin_inset Formula $a$
\end_inset

与
\begin_inset Formula $x$
\end_inset

无关, 
\begin_inset Formula $\bf{u=u}(x)$
\end_inset

.
 
\begin_inset Formula $\frac{\pd a\bf u}{\pd x}=a\frac{\pd\bf u}{\pd x}.$
\end_inset


\end_layout

\begin_layout Enumerate
常矩阵: 
\begin_inset Formula $\bf A$
\end_inset

与
\begin_inset Formula $x$
\end_inset

无关, 
\begin_inset Formula $\bf{u=u}(x)$
\end_inset

.
 
\begin_inset Formula $\frac{\pd\bf{Au}}{\pd x}=\bf A\frac{\pd\bf u}{\pd x}.$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
转置: 如果
\begin_inset Formula $\bf{u=u}(x),$
\end_inset

那么
\begin_inset Formula $\frac{\pd\bf u'}{\pd x}=\left(\frac{\pd\bf u}{\pd x}\right)'$
\end_inset

.
\end_layout

\begin_layout Enumerate
加法: 
\begin_inset Formula $\bf{u=u}(x),\bf{v=v}(x)$
\end_inset

.
 那么
\begin_inset Formula $\frac{\pd(\bf{u+v)}}{\pd x}=\frac{\pd\bf u}{\pd x}+\frac{\pd\bf v}{\pd x}.$
\end_inset


\end_layout

\begin_layout Enumerate
乘法相关
\end_layout

\begin_deeper
\begin_layout Enumerate
向量的外积: 
\begin_inset Formula $\bf{u=u}(x),\bf{v=v}(x).$
\end_inset

 
\begin_inset Formula $\frac{\pd(\bf{u'}\times\bf v)}{\pd x}=\left(\frac{\pd\bf u}{\pd x}\right)'\times\bf v+\bf u'\times\frac{\pd\bf v}{\pd x}.$
\end_inset


\end_layout

\begin_layout Enumerate
矩阵的外积: 
\begin_inset Formula $\frac{\pd(\bf{U\times v})}{\pd x}=\frac{\pd\bf U}{\pd x}\times\bf v+\bf U\times\frac{\pd\bf x}{\pd x}.$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
链式法则: 
\begin_inset Formula $\bf{u=u}(x),\bf{f=f(g),g=g(u).}$
\end_inset

那么
\begin_inset Formula $\frac{\pd\bf f}{\pd x}=\frac{\pd\bf f}{\pd\bf g}\frac{\pd\bf g}{\pd\bf u}\frac{\pd\bf u}{\pd x}.$
\end_inset


\end_layout

\begin_layout Subsection
向量对矩阵
\end_layout

\begin_layout Enumerate
常数相关: 
\begin_inset Formula $a$
\end_inset

不含
\begin_inset Formula $\bf X$
\end_inset

相关的变量, 
\begin_inset Formula $u=u(\bf X).$
\end_inset

 那么
\begin_inset Formula $\frac{\pd au}{\pd\bf X}=a\frac{\pd u}{\pd\bf x}.$
\end_inset


\end_layout

\begin_layout Enumerate
加法: 如果
\begin_inset Formula $u=u(\bf X),v=v(\bf X).$
\end_inset

 那么
\begin_inset Formula $\frac{\partial(u+v)}{\partial\mathbf{X}}=\frac{\partial u}{\partial\mathbf{X}}+\frac{\partial v}{\partial\mathbf{X}}$
\end_inset

.
\end_layout

\begin_layout Enumerate
乘法: 
\begin_inset Formula $u=u(\bf X),v=v(\bf X).$
\end_inset

 
\begin_inset Formula $\frac{\partial uv}{\partial\mathbf{X}}=u\frac{\partial v}{\partial\mathbf{X}}+v\frac{\partial u}{\partial\mathbf{X}}$
\end_inset

.
\end_layout

\begin_layout Enumerate
链式法则: 
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $u=u(\bf X)$
\end_inset

, 
\begin_inset Formula $\frac{\partial f(g(u))}{\partial\mathbf{X}}=\frac{\partial f(g)}{\partial g}\frac{\partial g(u)}{\partial u}\frac{\partial u}{\partial\mathbf{X}}.$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\mathbf{U}=\mathbf{U}(\mathbf{X})$
\end_inset

, 
\begin_inset Formula $\frac{\partial g(\mathbf{U})}{\partial X_{ij}}=\mathrm{tr}\left(\frac{\partial g(\mathbf{U})}{\partial\mathbf{U}}\frac{\partial\mathbf{U}}{\partial X_{ij}}\right)$
\end_inset


\end_layout

\end_deeper
\begin_layout Subsection
矩阵对标量
\end_layout

\begin_layout Enumerate
常数相关:
\end_layout

\begin_deeper
\begin_layout Enumerate
标量常数: 
\begin_inset Formula $\mathbf{U}=\mathbf{U}(x),a$
\end_inset

不是
\begin_inset Formula $x$
\end_inset

的函数: 
\begin_inset Formula $\frac{\partial a\mathbf{U}}{\partial x}=a\frac{\partial\mathbf{U}}{\partial x}.$
\end_inset


\end_layout

\begin_layout Enumerate
矩阵: 
\begin_inset Formula $\bf{A,B}$
\end_inset

不是
\begin_inset Formula $x$
\end_inset

的函数, 
\begin_inset Formula $\bf{U=U}(x)$
\end_inset

, 
\begin_inset Formula $\frac{\partial\mathbf{AUB}}{\partial x}=\mathbf{A}\frac{\partial\mathbf{U}}{\partial x}\mathbf{B}.$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
加法: 
\begin_inset Formula $\mathbf{U}=\mathbf{U}(x),\mathbf{V}=\mathbf{V}(x)$
\end_inset

, 
\begin_inset Formula $\frac{\partial(\mathbf{U}+\mathbf{V})}{\partial x}=\frac{\partial\mathbf{U}}{\partial x}+\frac{\partial\mathbf{V}}{\partial x}.$
\end_inset


\end_layout

\begin_layout Enumerate
乘法相关:
\begin_inset Formula $\bf{U=U}(x),\bf{V=V}(x)$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
矩阵乘法: 
\begin_inset Formula $\frac{\partial(\mathbf{U}\mathbf{V})}{\partial x}=\mathbf{U}\frac{\partial\mathbf{V}}{\partial x}+\frac{\partial\mathbf{U}}{\partial x}\mathbf{V}$
\end_inset

.
\end_layout

\begin_layout Enumerate
张量乘法: 
\begin_inset Formula $\frac{\partial(\mathbf{U}\otimes\mathbf{V})}{\partial x}=\mathbf{U}\otimes\frac{\partial\mathbf{V}}{\partial x}+\frac{\partial\mathbf{U}}{\partial x}\otimes\mathbf{V}.$
\end_inset


\end_layout

\begin_layout Enumerate
逐元素相乘: 
\begin_inset Formula $\frac{\partial(\mathbf{U}\circ\mathbf{V})}{\partial x}=\mathbf{U}\circ\frac{\partial\mathbf{V}}{\partial x}+\frac{\partial\mathbf{U}}{\partial x}\circ\mathbf{V}$
\end_inset


\end_layout

\begin_layout Enumerate
矩阵的逆: 
\end_layout

\begin_deeper
\begin_layout Enumerate
一元情况: 
\begin_inset Formula $\frac{\partial\mathbf{U}^{-1}}{\partial x}=-\mathbf{U}^{-1}\frac{\partial\mathbf{U}}{\partial x}\mathbf{U}^{-1}$
\end_inset


\end_layout

\begin_layout Enumerate
多元情况: 
\begin_inset Formula $\mathbf{U}=\mathbf{U}(x,y)$
\end_inset

, 
\begin_inset Formula $\frac{\partial^{2}\mathbf{U}^{-1}}{\partial x\partial y}=\mathbf{U}^{-1}\left(\frac{\partial\mathbf{U}}{\partial x}\mathbf{U}^{-1}\frac{\partial\mathbf{U}}{\partial y}-\frac{\partial^{2}\mathbf{U}}{\partial x\partial y}+\frac{\partial\mathbf{U}}{\partial y}\mathbf{U}^{-1}\frac{\partial\mathbf{U}}{\partial x}\right)\mathbf{U}^{-1}$
\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Enumerate
函数的复合: 
\begin_inset Formula $\bf A$
\end_inset

不是
\begin_inset Formula $x$
\end_inset

的函数, 
\begin_inset Formula $\bf{g(X)}$
\end_inset

是标量形式的多项式(可以是无穷级数的形式); 
\begin_inset Formula $g'(x)$
\end_inset

是对应的标量求导的结果; 
\begin_inset Formula $\bf{g'(x)}$
\end_inset

是对应矩阵的变化, 有
\begin_inset Formula $\frac{\partial\mathbf{g}(x\mathbf{A})}{\partial x}=\mathbf{A}\mathbf{g}^{\prime}(x\mathbf{A})=\mathbf{g}^{\prime}(x\mathbf{A})\mathbf{A}.$
\end_inset


\end_layout

